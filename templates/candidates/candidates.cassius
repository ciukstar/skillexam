
main
  img.photo
    border-radius: 50%

  div.no-content
    margin: 2rem
    display: flex
    flex-direction: column
    justify-content: center
    align-items: center
    gap: 2rem
    i.material-symbols-outlined
      font-size: 5rem

  #wrapperFabAdd
    position: fixed
    right: 1rem
    bottom: 1rem
    #fabAdd
      transition: visibility 0.3s linear, opacity 0.3s linear