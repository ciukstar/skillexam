
function sortExamsBy (dir,col) {
  const inputSortBy = document.getElementById(#{idInputSortBy});
  inputSortBy.name = dir;
  inputSortBy.value = col;
};

function filterExamsBy (col,val) {
  const inputResultFilter = document.getElementById(#{idInputResultFilter});
  inputResultFilter.name = col;
  inputResultFilter.value = val;
};
